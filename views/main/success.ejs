          >
            <div class="row d-flex justify-content-center text-center">
              <div class="col-md-10">
                <!-- Heading -->

                <!-- Divider -->
                <hr class="hr-light" />

                <!-- Description -->
                
                <h4 style="color: white;">Successfully Added Treasure to The Trail!</h4>
                <hr class="hr-light" />
                <a
                href="/api/trails/all/<%= lat %>/<%= lon %>  "
                class="btn btn-success" 
                >Back To Search Results</a>
                
              </div>
            </div>
